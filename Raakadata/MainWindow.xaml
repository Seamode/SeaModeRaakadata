<Window x:Class="Raakadata.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Raakadata"
        mc:Ignorable="d"
        MinHeight="400" MinWidth="650"
        Title="MainWindow" Height="500" Width="800">
    <Grid Margin="10" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="173*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Image Grid.RowSpan="7" Grid.ColumnSpan="24" Source="asets/Seamode.jpg" />
        <Label Grid.Column="0" Grid.Row="8" Content="Path:" Padding="0" VerticalAlignment="Center"/>
        <TextBox Grid.Column="2" Grid.Row="8" Grid.ColumnSpan="7" Text="C:/" VerticalAlignment="Center"/>
        <Button Grid.Column="10" Grid.Row="8" Content="Open" VerticalAlignment="Center"/>
        <TextBox Grid.Column="14" Grid.Row="8" Grid.ColumnSpan="7" Text="C:/" VerticalAlignment="Center"/>
        <Button Grid.Column="22" Grid.Row="8" Content="Open" VerticalAlignment="Center" />
        <TextBox Grid.Column="2" Grid.Row="12" Grid.ColumnSpan="9" Text="C:/SeaMODE_20190928_112953.csv&#xA;C:/SeaMODE_20190928_125827.csv&#xA;C:/SeaMODE_20190928_142718.csv" Grid.RowSpan="5" IsReadOnly="True" />
        <TextBox Grid.Column="14" Grid.Row="12" Grid.ColumnSpan="9" Text="C:/SeaMODE_20190928_142718_KILPAILU.csv" Grid.RowSpan="5" IsReadOnly="True" />
        <Label Grid.Column="2" Grid.Row="7" Content="Parce From:" Padding="0" Grid.ColumnSpan="7" VerticalAlignment="Center"/>
        <Label Grid.Column="14" Grid.Row="7" Content="Parce To:" Padding="0" Grid.ColumnSpan="7"  VerticalAlignment="Center"/>
        <Label Grid.Column="6" Grid.Row="10" Content="End date:" Padding="0" Grid.ColumnSpan="1"  VerticalAlignment="Center"/>
        <Label Grid.Column="0" Grid.Row="10" Content="Start date:" Padding="0" Grid.ColumnSpan="1"  VerticalAlignment="Center"  />
        <Label Grid.Column="14" Grid.Row="10" Content="Output file:" Padding="0" Grid.ColumnSpan="7" VerticalAlignment="Center" />
        <Label Grid.Column="0" Grid.Row="18" Content="Errors:" Padding="0" VerticalAlignment="Center" />
        <Label Grid.Column="2" Grid.Row="18" Content="No errors" Padding="0" Grid.ColumnSpan="7" VerticalAlignment="Center"/>
        <Button Grid.Column="14" Grid.Row="18" Content="Parce raw data"  Grid.ColumnSpan="3" VerticalAlignment="Center"/>
        <Button Grid.Column="18" Grid.Row="18" Content="Parce GPX data"  Grid.ColumnSpan="3" VerticalAlignment="Center"/>
        <TextBox Grid.Column="1"  Grid.Row="7"  Text="i"  ToolTipService.InitialShowDelay="2000" ToolTipService.ShowDuration="20000" ToolTipService.BetweenShowDelay="10000" ToolTip="This is a tool tip."  Foreground="Red"  VerticalAlignment="Center" />
        <TextBox Grid.Column="13"  Grid.Row="7"  Text="i"  ToolTipService.InitialShowDelay="2000" ToolTipService.ShowDuration="20000" ToolTipService.BetweenShowDelay="10000" ToolTip="This is a tool tip."  Foreground="Red"  VerticalAlignment="Center" />
        <DatePicker Grid.Column="2"  Grid.Row="10"  Grid.ColumnSpan="3" VerticalAlignment="Center"/>
        <DatePicker Grid.Column="8"  Grid.Row="10"  Grid.ColumnSpan="3" VerticalAlignment="Center"/>



    </Grid>
</Window>
